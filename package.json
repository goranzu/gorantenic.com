{
  "name": "eleventy-starter",
  "version": "1.0.0",
  "description": "elventy starter",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "run-s clean eleventy:dev --print-label",
    "build": "run-s clean eleventy:prod --print-label",
    "eleventy:dev": "ELEVENTY_ENV=development eleventy --serve",
    "eleventy:prod": "ELEVENTY_ENV=production eleventy",
    "clean": "rimraf dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Goran Tenic",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-navigation": "^0.1.6",
    "@11ty/eleventy-plugin-rss": "^1.1.0",
    "clean-css": "^5.1.0",
    "critical": "^2.0.6",
    "cssesc": "^3.0.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "html-minifier": "^4.0.0",
    "inline-source": "^7.2.0",
    "markdown-it": "^12.0.4",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.39.1",
    "rollup-plugin-terser": "^7.0.2"
  }
}
